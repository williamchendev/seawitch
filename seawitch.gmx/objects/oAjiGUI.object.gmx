            <string>///Draw GUI

//Water Display
WaterLevel = round(instance_number(oWaterCircle) div 2);
WaterLevelx = view_xview + 10;
WaterLevely = view_yview + 10;

if (WaterLevel &gt;= 50){
    WaterLevelDisplay = 50;
}
if (WaterLevel &lt; 0){
    WaterLevelDisplay = 0;
}
else{
    WaterLevelDisplay = WaterLevel;
}

draw_sprite(sWaterStatus, 55, WaterLevelx, WaterLevely);
draw_sprite(sWaterStatus, WaterLevelDisplay, WaterLevelx, WaterLevely);

//Draw Canteens
Canteenx = 16;
Canteeny = 12;

for (i = 0; i &lt; oPlayer.Canteens; i++){
    draw_sprite(sCanteen, 0, WaterLevelx + 56 + (Canteenx * i), WaterLevely + Canteeny);
}

//Draw Endurance
draw_set_color(make_color_rgb(26, 102, 46));
draw_rectangle(WaterLevelx + 56, WaterLevely + 24,  round(oPlayer.Endurance * 0.5) + WaterLevelx + 56, WaterLevely + 26, false);
draw_set_color(c_white);

//Draw Wheel Menu
if (oPlayer.ITEMS and selecting){
    draw_sprite(sGUI, 0, x, y);
    if (menuselect != -1){
        draw_sprite(sGUI, menuselect, x, y);
    }
}
