            <string>///Main Step event

//Place with player
x = oAji.x;
y = oAji.y;
hspeed = oAji.hspeed;
vspeed = oAji.vspeed;

//Draw Wheel Menu
if (oPlayer.ITEMS and oPlayer.canmove and !selecting){
    freezeplayer();
    selecting = true;
    oPlayer.gui = true;
}
else if (oPlayer.ITEMS and selecting){
    freezeplayer();
    
    if (oPlayer.RIGHTKey and oPlayer.DOWNKey){
        menuselect = 2;
    }
    else if (oPlayer.RIGHTKey){
        menuselect = 1;
    }
    else if (oPlayer.DOWNKey){
        menuselect = 3;
    }
    else if (oPlayer.LEFTKey == -1){
        menuselect = 4;
    }
    else if (oPlayer.UPKey){
        menuselect = 5;
    }
    else {
        menuselect = -1;
    }
}
else if (!oPlayer.ITEMS and selecting and menuselect != -1){
    if (menuselect == 4){
        oWeapons.show = true;
    }
    else if (menuselect != -1){
        oInventory.show = true;
        oInventory.sidebaralpha = 0;
    }
    selecting = false;
    menuselect = -1;
}
else if (!oPlayer.ITEMS and selecting and menuselect == -1){
    unfreezeplayer();
    selecting = false;
    oPlayer.gui = false;
}
</string>
